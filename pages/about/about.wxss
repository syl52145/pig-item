/* pages/about/about.wxss */
page{
    padding:20rpx;
    box-sizing: border-box;
    background: #fcfcfc;
}
.news{
    width: 100%;
    padding:20rpx;
    box-sizing: border-box;
    background: #ddd;
    border-radius: 5rpx;
}
.news .title{
    height: 30px;
    width: 100%;
    border-bottom: 1px solid #fcfcfc;
}
.news .content{
    font-size: 14px;
    padding:20rpx 0;
    color: #666;
    margin-bottom: 20rpx;
    border-bottom: 1px solid #fcfcfc;
}
.news .footer{
    font-size: 12px;
    color: #666;
    display: flex;
    justify-content: space-between;
}
